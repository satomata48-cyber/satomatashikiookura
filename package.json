{
  "name": "satomatashikiookura",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev .svelte-kit/cloudflare --persist-to .wrangler/state --compatibility-flag nodejs_compat --d1 DB",
    "db:init": "wrangler d1 execute DB --local --persist-to .wrangler/state --file=./schema.sql"
  },
  "dependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "svelte": "^5.45.2",
    "vite": "^7.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "wrangler": "^4.51.0"
  },
  "optionalDependencies": {
    "@cloudflare/workerd-windows-64": "^1.20251128.0"
  }
}
